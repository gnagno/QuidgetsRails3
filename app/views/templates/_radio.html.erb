<input type="radio"
  name="<%= html_options[:name] %>" 
  id="<%= html_options[:id] %>" 
  class="ph-radio <%= html_options[:class] %>"
  value="<%= choice_id %>"
  data-model-name="<%= object_model %>"
  data-model-id="<%= object_id %>"  
  data-choice-model="<%= choice_model %>"
  data-callback="<%= callback %>"
   <%= " checked " if is_checked %>
/>
